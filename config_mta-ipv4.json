{
    "multiprocessing": true,
    "max_concurrency": 32,    
    "input_pcap_path": "./input_mta/",
    "sort_by_filesize": false,
    "output_path": "output_mta-ipv4_unsorted",
    "output_type": "csv",
    "output_stdout_fileinfo": false,
    "output_stdout_print_granularities": "",    
    "plugins_path": "./build/_deps/fpnt-build/plugins/libFPNT_PLUGINS.so",
    "force_remove": true,
    "dfref_path": "dfref",
    "granularities": "pkt,flow,flowset",
    "genKey_pkt": "genKey_pkt_default",
    "genKey_flow": "genKey_flow_ipv4",
    "genKey_flowset": "genKey_flowset_ipv4",
    "early_stop_pkts": -1,
    "configcsv_path": "config_mta-ipv4",
    "tshark_path": "tshark",
    "tshark_displayfilter": "ssl||tcp||udp",
    "tshark_option": "-V",
    "fpnt_tshark_error_log": "/tmp/fpnt_tshark_err.log",
    "log_numbering_concurrency": true,
    "_comment1": "output directory must be an empty directory. otherwise, it will be deleted.",
    "_comment2": "early_stop_pkts will not work if it is -1"
}
